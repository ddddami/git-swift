{
	"name": "git-swift",
	"version": "0.1.5",
	"description": "A fast git branch switcher with fuzzy search",
	"main": "index.js",
	"scripts": {
		"postinstall": "golang-npm install",
		"preuninstall": "golang-npm uninstall"
	},
	"goBinary": {
		"name": "git-swift",
		"path": "./bin",
		"url": "https://github.com/ddddami/git-swift/releases/download/v{{version}}/git-swift_{{platform}}_{{arch}}.tar.gz"
	},
	"dependencies": {
		"golang-npm": "^0.0.6"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ddddami/git-swift.git"
	},
	"keywords": ["git", "swift", "branch", "switch", "go", "cli", "terminal"],
	"author": "Damilola O.",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ddddami/git-swift/issues"
	},
	"homepage": "https://github.com/ddddami/git-swift#readme",
	"engines": {
		"node": ">=12"
	}
}
